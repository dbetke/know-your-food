extends layout

block navbar
  ul.nav.nav-pills.pull-right
    li
      a(href='../') Home
    li
      a(href='/search') Search  
    li
      a(href='/contribute') Contribute
    li
      a(href='/about') About
    li
      a(href='/contact') Contact
  div.spacer

block content  
  div
    h4 Search Results
    p

block results

  -for result in results
    b=result.brandname + " " + result.productname 
    p=result.ingredients